{
    "seed": 2,
    
    "dataset": "pacs",
    "test_domain": ["photo"], 
    
    "local_json_dir_name": "tmp",
    
    "learning_rate": 0.0001,
    "decay_every": 500,
    "decay_base": 0.85,
    "batch_size": 32,
    "num_epochs": 30,
    "alpha": 0.5,
    "lambd": 0.75,
    
    "l2_penalty_weight": 1e-05,
    
    "eval_every_n_epochs": 5,
    "save_summary_steps": 500,
    "save_checkpoints_secs": 600,
    "log_step_count_steps": 500,
    

    "num_classes": 7,
    "name_classifier": "ResNet50",
    "name_critic": "critic",
    "name_generator": "generator",
    "resnet_weights": "imagenet",

    "dropout_rate": 0.7, 
    "use_dropout": 1
}
