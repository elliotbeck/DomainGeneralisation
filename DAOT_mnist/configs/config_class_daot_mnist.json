{
    "seed": 1,
    
    "dataset": "mnist",
    "local_json_dir_name": "tmp",
    
    "learning_rate": 0.000001,
    "decay_every": 500,
    "decay_base": 0.85,
    "batch_size": 8,
    "num_epochs": 2,
    "alpha": 0.5,
    "lambd": 0.5,
    
    "l2_penalty_weight": 1e-05,
    
    "eval_every_n_epochs": 5,
    "save_summary_steps": 500,
    "save_checkpoints_secs": 600,
    "log_step_count_steps": 500,
    

    "num_classes": 2,
    "name_classifier": "basic_nn",
    "name_critic": "critic",
    "name_generator": "generator",
    "resnet_weights": "imagenet",

    "dropout_rate": 0.7, 
    "use_dropout": 1
}
