{
    "seed": 1,
    
    "dataset": "pacs",
    "test_domain": ["art_painting"], 
    "training_domains": ["cartoon", "photo", "sketch"],    

    "local_json_dir_name": "tmp",
    
    "learning_rate": 0.0001,
    "decay_every": 500,
    "decay_base": 0.85,
    "batch_size": 18,
    "num_epochs": 1,
    "epsL": 0.5,
    "epsD": 0.5,

    
    "l2_penalty_weight": 0,
    
    "eval_every_n_epochs": 5,
    "save_summary_steps": 500,
    "save_checkpoints_secs": 600,
    "log_step_count_steps": 500,
    

    "num_classes_label": 7,
    "num_classes_domain": 3,
    "name_classifier_domain": "classifier_domain",
    "name_classifier_label": "classifier_label",
    "resnet_weights": "imagenet",

    "dropout_rate": 0.7, 
    "use_dropout": 1
}
